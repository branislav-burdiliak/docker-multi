{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "branislavburdiliak/docker-multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "branislavburdiliak/docker-multi-nginx",
      "hostname": "nginx",
      "essential": true
    },
    {
